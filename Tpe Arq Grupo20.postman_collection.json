												"exec": [
													"pm.test(\"Status es 2xx\", function () {\r",
													"  pm.expect(pm.response.code).to.be.within(200, 299);\r",
													"});\r",
													"\r",
													"pm.test(\"Content-Type es JSON\", function () {\r",
													"  pm.response.to.have.header(\"Content-Type\");\r",
													"  const ct = pm.response.headers.get(\"Content-Type\");\r",
													"  pm.expect(ct).to.include(\"application/json\");\r",
													"});\r",
													"\r",
													"if (pm.response.code >= 400) {\r",
													"  console.log(\"Status:\", pm.response.code, \"Body:\", pm.response.text());\r",
													"  pm.test(\"La API no debería responder 4xx/5xx en /usuarios\", function () {\r",
													"    pm.expect.fail(`Respuesta ${pm.response.code} para ${pm.request.url}`);\r",
													"  });\r",
													"}\r",
													"\r",
													"pm.test(\"Body es JSON válido\", function () {\r",
													"  pm.expect(() => pm.response.json()).to.not.throw();\r",
													"});\r",
													"\r",
													"pm.test(\"Respuesta es array de usuarios\", function () {\r",
													"  const data = pm.response.json();\r",
													"  pm.expect(data).to.be.an(\"array\");\r",
